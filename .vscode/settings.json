{
  "editor.formatOnSave": true,
  "[toml]": {
    "editor.formatOnSave": false
  },
  "[django-html]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.py-tpl": "python",
    "*.html": "django-html",
    "*.js": "javascript",
    "*.svg": "django-html"
  },
  "cSpell.words": [
    "bookformat",
    "bookreading",
    "imagekit",
    "notisbn",
    "olclient",
    "openlibrary",
    "userbook"
  ],
  "python.analysis.exclude": [".venv"],
  "python.testing.pytestEnabled": true
}
